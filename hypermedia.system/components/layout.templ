package components

templ Layout(title string, contents []templ.Component) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
			<title>{ title }</title>
			<script src="./js/htmx.min.js"></script>
		</head>
		<body>
			for _, content := range contents {
				@content
			}
		</body>
	</html>
}
